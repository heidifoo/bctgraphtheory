% one output - local efficiency 
loceff = fopen('schaefer_fz_local_efficiency_bin010.csv','w');
files = dir('*_schaefer.netcc')
for f = files' 
    x = importdata(f.name);
    binary_matrix = get_binary_matrix(x,0.10);
    output = efficiency_bin(binary_matrix,1);
    str_to_print = strcat(f.name,',',join(string(output),','),'\n');
    fprintf(loceff,str_to_print);
end 
